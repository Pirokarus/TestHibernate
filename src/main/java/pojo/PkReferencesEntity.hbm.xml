<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.PkReferencesEntity" table="PK_REFERENCES" schema="OLIM">
        <many-to-one name="pkObjectsByObjectId" class="pojo.PkObjectsEntity">
            <column name="OBJECT_ID"/>
        </many-to-one>
        <many-to-one name="pkAttributesByAttributeId" class="pojo.PkAttributesEntity">
            <column name="ATTRIBUTE_ID"/>
        </many-to-one>
        <many-to-one name="pkObjectsByReferenceId" class="pojo.PkObjectsEntity">
            <column name="REFERENCE_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>