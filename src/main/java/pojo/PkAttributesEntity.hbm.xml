<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.PkAttributesEntity" table="PK_ATTRIBUTES" schema="OLIM">
        <id name="attributeId">
            <column name="ATTRIBUTE_ID" sql-type="number(*)" precision="2147483646"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar2(255)"/>
        </property>
        <property name="details">
            <column name="DETAILS" sql-type="varchar2(255)" not-null="true"/>
        </property>
        <property name="ismutible">
            <column name="ISMUTIBLE" sql-type="number(1)" precision="1" not-null="true"/>
        </property>
        <many-to-one name="pkAttributeTypesByAttributeTypeId" class="pojo.PkAttributeTypesEntity">
            <column name="ATTRIBUTE_TYPE_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>